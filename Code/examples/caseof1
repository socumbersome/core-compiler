//main = Pack{2,4} 5 2 (Pack{1,3} (Pack{3,0} ) 9 8) 1 
f x = Pack{2,2} (case (t x (1+7)) of {1} -> 1, {2} -> 2) Pack{1,0} ;
prefix p xs = map (Pack{2,2} p) xs ;
main = length (cons 1 (cons 2 nil))
